{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch Native",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/bin/llvmta",
            "args": [
                "-disable-tail-calls",
                "-float-abi=hard",
                "-mattr=-neon,+vfp2",
                "-O0",
                "--ta-muarch-type=inorder",
                "--ta-memory-type=separatecaches",
                "--ta-strict=false",
                "--ta-loop-bounds-file='${workspaceRoot}/testcases/test/LoopAnnotations.csv'",
                "--ta-loop-lowerbounds-file='${workspaceRoot}/testcases/test/LLoopAnnotations.csv'",
                "--ta-num-callsite-tokens=1",
                "--core-info=${workspaceRoot}/testcases/test/CoreInfo.json",
                "--core-numbers=2",
                "--shared-cache-Persistence-Analysis=false",
                "--ta-l2cache-persistence=elementwise",
                "-debug-only=",
                "optimized.ll"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/testcases/dirforgdb",
            "environment": [
                {
                    "name": "GUROBI_HOME",
                    "value": "${workspaceRoot}/thirdParty/gurobi952/linux64"
                },
                {
                    "name": "PATH",
                    "value": "${env:PATH}:${workspaceRoot}/thirdParty/gurobi952/linux64/bin"
                },
                {
                    "name": "GRB_LICENSE_FILE",
                    "value": "${workspaceRoot}/thirdParty/gurobi952/gurobi.lic"
                },
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${env:LD_LIBRARY_PATH}:${workspaceRoot}/thirdParty/gurobi952/linux64/lib"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "run gdb"
        },
        {
            "name": "(gdb) Launch Docker",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/bin/llvmta",
            "args": [
                "-disable-tail-calls",
